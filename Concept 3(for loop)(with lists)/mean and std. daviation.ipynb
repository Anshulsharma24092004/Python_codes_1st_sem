    "list1 = [5,5,5,9,9,9,10,10,10]\n",
    "x=0\n",
    "for i in list1:\n",
    "    x=x+i\n",
    "mean1 = x/len(list1)\n",
    "print(mean1)\n",
    "h=0\n",
    "for i in list1:\n",
    "    k=i-mean1\n",
    "    m=k**2\n",
    "    h=h+m\n",
    "p = h/((len(list1))-1)\n",
    "dev = p**0.5\n",
    "print(dev)\n"
   ]
